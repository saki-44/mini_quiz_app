<%= form_with url: answer_question_path(@question), method: :post, local: true do |form| %>
  <h1><%= @question.content %></h1>
  <% @question.choices.each do |choice| %>
    <li>
      <%= radio_button_tag 'choice_id', choice.id %>
      <%= label_tag "choice_#{choice.id}", choice.content %>
    </li>
  <% end %>
  <%= form.submit "回答する" %>
<% end %>



